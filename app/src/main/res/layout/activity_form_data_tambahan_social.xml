                    app:srcCompat="@drawable/ic_map_24_px" />

                <TextView
                    android:id="@+id/cityLabel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/cityIcon"
