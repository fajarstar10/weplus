                    app:srcCompat="@drawable/ic_calendar_24px" />

                <TextView
                    android:id="@+id/addressLabel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/addressIcon"
