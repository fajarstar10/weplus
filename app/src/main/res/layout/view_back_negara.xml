<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="?attr/actionBarSize"
    android:background="@color/red">

    <androidx.appcompat.widget.SearchView
        android:id="@+id/srch_negara"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_11"
        android:layout_marginTop="@dimen/margin_6"
        android:layout_marginRight="@dimen/margin_75"
        android:layout_marginBottom="@dimen/margin_6"
        android:background="@drawable/border_white">
    </androidx.appcompat.widget.SearchView>

    <LinearLayout
        android:id="@+id/batal_negara"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_centerHorizontal="true"
        android:gravity="center_vertical">

        <TextView
            android:id="@+id/viewback_title_negara"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:textStyle="bold"
            android:fontFamily="@font/ubuntu_l"
            android:textSize="@dimen/font_medium"
            android:layout_toRightOf="@id/srch_negara"
            android:layout_marginRight="@dimen/margin_20"
            android:text="Batal"
            android:textColor="@android:color/white"
            android:layout_gravity="right"/>


    </LinearLayout>


</RelativeLayout>